{
  "name": "paypulse99",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "next dev --turbo",
    "dev:debug": "next dev --inspect",
    "auto-dev": "next dev --yes",
    "auto-dev:fast": "next dev --turbo --yes",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:debug": "next start --inspect",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out dist",
    "clean:win": "if exist .next rmdir /s /q .next && if exist out rmdir /s /q out && if exist dist rmdir /s /q dist",
    "reset": "npm run clean && npm install && npm run dev"
  },
  "dependencies": {
    "exceljs": "^4.4.0",
    "lucide-react": "^0.294.0",
    "next": "^15.4.6",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.3.8",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}